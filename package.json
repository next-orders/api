{
  "name": "api",
  "version": "0.1.3",
  "description": "Frontend needs data from backend: categories, products, checkouts, etc.",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prisma:push": "prisma db push",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json"
  },
  "keywords": [
    "ecommerce",
    "api",
    "nestjs",
    "express",
    "prisma"
  ],
  "author": "Nick Kosarev <hmbanan666@hotmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/next-orders/api.git"
  },
  "bugs": {
    "url": "https://github.com/next-orders/api/issues"
  },
  "homepage": "https://github.com/next-orders/api#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "3.462.0",
    "@dicebear/core": "7.0.1",
    "@dicebear/open-peeps": "7.0.1",
    "@nestjs/common": "10.2.10",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.10",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/platform-express": "10.2.10",
    "@paralleldrive/cuid2": "2.2.2",
    "@prisma/client": "5.6.0",
    "bcrypt": "5.1.1",
    "nestjs-zod": "3.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "sharp": "0.33.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.1",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.5.0",
    "prettier": "^3.1.0",
    "prisma": "5.6.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.1",
    "typescript": "5.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
